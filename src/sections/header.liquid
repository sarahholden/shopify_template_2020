<div data-section-id="{{ section.id }}" data-section-type="header-section">
  <div class="nav-wrapper" role="banner">

    <div class="close-mobile-nav"></div>

    <div class="container">
      <div class="inner">
        <!--   LOGO   -->
        {%- include 'nav-logo' -%}

        <!--   NAV   -->
        {%- include 'nav-menu' -%}

        <!-- Social Icons -->
        {%- include 'social-icons' -%}

        <!-- SEARCH -->
        <!-- {%- include 'nav-search' -%} -->
        <!-- ACCOUNT LINKS -->
        <!-- {% if shop.customer_accounts_enabled %}
          {%- include 'nav-account-links' -%}
        {% endif %} -->

        <div class="hamburger">
          <span></span>
          <span></span>
          <span></span>
        </div>

        <!--   CART   -->
        {%- include 'nav-cart-link' -%}
      </div>
    </div>



  </div>
</div>

{% schema %}
  {
    "name": "Header",
    "settings": [
      {
        "type": "url",
        "id": "logo",
        "label": "Logo image"
      },
      {
        "type": "link_list",
        "id": "main_linklist",
        "label": "Menu",
        "default": "main-menu"
      }
    ]
  }
{% endschema %}
